  "devDependencies": {
    "@loopback/testlab": "^7.0.6",
    "@nutol/build": "^0.2.22",
    "@nutol/eslint-config": "^0.2.16",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.6.1",
    "@types/uuid": "^10.0.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0"
