{
  "name": "@bleco/audit-log",
  "description": "A loopback-next audit Log extension",
  "version": "1.0.9",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./sequelize": {
      "types": "./dist/index.sequelize.d.ts",
      "default": "./dist/index.sequelize.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "author": "ty",
  "license": "MIT",
  "publishConfig": {
