{
  "name": "@bleco/boot",
  "description": "A loopback boot extension library to allow boot from '*.ts' files",
  "version": "0.3.2",
  "keywords": [
    "loopback",
    "boot",
    "extension",
    "typescript",
    "bleco"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "ty",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nb-tsc",
    "clean": "nb-clean 'bleco-boot*.tgz' 'dist/*' '*.tsbuildinfo' 'package'",
    "pretest": "yarn run clean && yarn run build",
    "test": "yarn run pretest && yarn run jest",
    "jest": "jest"
  },
  "dependencies": {
    "@loopback/boot": "^5.0.8"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.13",
    "@nutol/eslint-config": "^0.2.8",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
