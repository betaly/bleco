{
  "name": "@bleco/example-oidp-server",
  "description": "An example OIDP server for LoopBack 4",
  "version": "0.4.1",
  "private": true,
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "author": "ty",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "posttest": "yarn run lint",
    "docker:build": "docker build -t jumper .",
    "docker:run": "docker run -p 3000:3000 -d jumper",
    "premigrate": "yarn run build",
    "migrate": "yarn premigrate && node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "yarn preopenapi-spec && node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "yarn prestart && node -r source-map-support/register .",
    "clean": "lb-clean 'dist' '*.tsbuildinfo' '.eslintcache'",
    "rebuild": "yarn clean && yarn build",
    "test": "yarn pretest &&  yarn jest && yarn posttest",
    "jest": "jest",
    "test:ci": "jest --collectCoverage --coverageDirectory=\"./coverage\" --reporters=default --reporters=jest-junit --watchAll=false"
  },
  "dependencies": {
    "@bleco/boot": "^0.5.1",
    "@bleco/config": "^0.7.1",
    "@bleco/oidp": "^0.5.1",
    "@loopback/core": "^5.1.2",
    "@loopback/repository": "^6.1.2",
    "@loopback/rest": "^13.1.2",
    "@loopback/rest-explorer": "^6.1.2",
    "@loopback/service-proxy": "^6.1.2",
    "bcryptjs": "^2.4.3",
    "berrors": "^0.2.3",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "tily": "^2.21.1"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.2",
    "@loopback/eslint-config": "^14.0.3",
    "@loopback/testlab": "^6.1.2",
    "@types/bcryptjs": "2.4.3",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "jest": "^29.6.4"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
